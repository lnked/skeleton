# editorconfig.org
root = true

[*]
indent_style = space
end_of_line = lf
charset = Windows-1251
trim_trailing_whitespace = true
insert_final_newline = true

[*.{json,js,jsx,html,css}]
indent_style = space
indent_size = 4

[.eslintrc]
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespace = false
